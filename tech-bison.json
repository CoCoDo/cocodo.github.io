{
	"version": "3.0.4",
	"uri": "https://www.gnu.org/software/bison/",
	"logo": "gnu.png",
	"download": "http://ftp.gnu.org/gnu/bison/",
	"parsing": ["LALR(1)", "GLR"],
	"more": "https://www.gnu.org/software/bison/manual/bison.html",
	"example": [
		"%token TYPE DOTDOT ID",
		"",
		"%left '+' '-'",
		"%left '*' '/'",
		"",
		"%%",
		"type_decl: TYPE ID '=' type ';' ;",
		"",
		"type: '(' id_list ')'",
		"    | expr DOTDOT expr;",
		"id_list: ID",
		"       | id_list ',' ID;",
		"expr: '(' expr ')'",
		"    | expr '+' expr",
		"    | expr '-' expr",
		"    | expr '*' expr",
		"    | expr '/' expr",
		"    | ID;"],
	"examplesrc": "https://www.gnu.org/software/bison/manual/bison.html",
	"maintained": "Akim Demaille and Paul Eggert",
	"wikipedia": {"GNU bison": ["en"], "GNU Bison": ["fr", "ru", "de"]}
}